<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.voteapp">

    <uses-permission android:name="android.permission.USE_BIOMETRIC" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.voteapp.FinalActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity android:name="com.example.voteapp.UserUpdatePassword" />
        <activity android:name="com.example.voteapp.SelectParty" />
        <activity android:name="com.example.voteapp.VerifiyId" />
        <activity
            android:name="com.example.voteapp.HomeActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity android:name="com.example.voteapp.RegisterActivity" />
        <activity android:name="com.example.voteapp.LoginActivity" />
        <activity android:name="com.example.voteapp.welcomeActivity" />
        <activity android:name="com.example.voteapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>